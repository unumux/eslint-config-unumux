{
    "name": "@unumux/eslint-config-unumux",
    "description": "",
    "main": "browser.js",
    "scripts": {
        "commit": "git-cz",
        "test": "true"
    },
    "version": "0.0.0-SEMANTICALLY-RELEASED",
    "author": "",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://github.com/unumux/eslint-config-unumux.git"
    },
    "devDependencies": {
        "@types/eslint": "^4.16.0",
        "@types/prettier": "^1.10.0",
        "@unumux/eslint-config-unumux": "^1.1.0",
        "commitizen": "^2.5.0",
        "cz-conventional-changelog": "^2.1.0",
        "eslint": "^4.18.2",
        "ghooks": "^2.0.2",
        "prettier": "^1.11.1",
        "semantic-release": "^17.2.3",
        "validate-commit-msg": "^2.14.0"
    },
    "config": {
        "ghooks": {
            "commit-msg": "validate-commit-msg"
        },
        "validate-commit-msg": {
            "types": [
                "feat",
                "fix",
                "docs",
                "style",
                "refactor",
                "perf",
                "test",
                "chore",
                "revert",
                "ci"
            ],
            "warnOnFail": false,
            "maxSubjectLength": 100
        },
        "commitizen": {
            "path": "node_modules/cz-conventional-changelog"
        }
    },
    "dependencies": {}
}
